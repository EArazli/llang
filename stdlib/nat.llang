import "./struct.cartesian.llang";

doctrine Nat extends StructCartesian where {
  type Nat @M;

  gen Z : [] -> [Nat] @M;
  gen S : [Nat] -> [Nat] @M;
  gen add : [Nat, Nat] -> [Nat] @M;

  rule computational addZ -> : [Nat] -> [Nat] @M =
    (Z * id[Nat]) ; add == id[Nat]

  rule computational addS -> : [Nat, Nat] -> [Nat] @M =
    (S * id[Nat]) ; add == add ; S
}
