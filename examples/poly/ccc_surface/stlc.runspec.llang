import "./stlc.surface.llang";

polymodel CCCSymbolic : CCC where {
  default = symbolic;

  op dup(x) = [x, x];
  op drop(x) = [];
  op swap(x, y) = [y, x];
}

polyrun_spec STLCinCCC where {
  doctrine CCC;
  surface STLC;
  model CCCSymbolic;
  policy UseOnlyComputationalLR;
  fuel 200;
  show normalized;
}
