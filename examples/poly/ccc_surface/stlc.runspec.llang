import "./ccc.poly.llang";
import "../../ccc_surface/stlc.surface.llang";
import "../../ccc_surface/stlc.syntax.llang";

polymodel CCCSymbolic : CCC where {
  default = symbolic;

  op dup(x) = [x, x];
  op drop(x) = [];
  op swap(x, y) = [y, x];
}

polyrun_spec STLCinCCC where {
  doctrine CCC;
  core_doctrine CCC;
  surface STLC;
  surface_syntax STLC_Syntax;
  model CCCSymbolic;
  policy UseOnlyComputationalLR;
  fuel 200;
  show normalized;
  show cat;
  show value;
}
