import "./console.llang";

surface MiniFun where {
  doctrine ConsoleExpr;
  mode M;

  lexer {
    keywords: ;
    symbols: "(", ")", ",";
  }

  expr {
    atom:
      ident(name) "(" <expr> ")" => $1 ; #name
    | ident(name) => $name
    | "(" <expr> ")" => <expr>
    ;

    infixr 10 "," => $1 * $2;
  }
}
